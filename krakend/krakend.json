        }
      ]
    },
    {
      "endpoint": "/moneydashboard/api/v1/operations/{uuid}/bank-account/{bankAccountUuid}",
      "input_headers": [
        "Content-Type",
        "uuid"
      ],
      "method": "PUT",
      "backend": [
        {
          "host": [
            "http://moneydashboard_backend:8080"
          ],
          "url_pattern": "/operations/{uuid}/bank-account/{bankAccountUuid}"
        }
      ]
    },
    {
      "endpoint": "/moneydashboard/api/v1/operations/{uuid}/bank-account/{bankAccountUuid}",
      "input_headers": [
        "uuid"
      ],
      "method": "DELETE",
      "backend": [
        {
          "host": [
            "http://moneydashboard_backend:8080"
          ],
          "url_pattern": "/operations/{uuid}/bank-account/{bankAccountUuid}"
        }
      ]
    }
  ]
}