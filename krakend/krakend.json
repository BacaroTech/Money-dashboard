      "input_headers": [
        "uuid"
      ],
      "method": "DELETE",
      "backend": [
        {
          "host": [
            "http://moneydashboard_backend:8080"
          ],
          "url_pattern": "/bank-accounts/{uuid}"
        }
      ]
    },
    {
      "endpoint": "/moneydashboard/api/v1/bank-accounts/{uuid}",
      "input_headers": [
        "Content-Type",
        "uuid"
      ],
      "method": "PUT",
      "backend": [
        {
          "host": [
            "http://moneydashboard_backend:8080"
          ],
          "url_pattern": "/bank-accounts/{uuid}"
        }
